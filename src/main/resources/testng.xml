<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Suite" verbose="1">
    <listeners>
        <listener class-name="com.cheapflights.common.listeners.TestsListener"/>
    </listeners>
    <test name="MoscowToTokyo">
        <classes>
            <class name="com.cheapflights.ui.tests.CheapFlightsTest">
                <parameter name="file" value="./src/main/resources/TravelInfo.json"></parameter>
            </class>
        </classes>
    </test>
    <test name="Choosing flight from Moscow to Tokyo">
        <classes>
            <class name="com.cheapflights.ui.tests.CheapFlightsTest">
                <parameter name="file" value="./src/main/resources/TravelInfo2.json"></parameter>
            </class>
        </classes>
    </test>

</suite>